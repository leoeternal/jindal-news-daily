        <div class="container">
            <h4>About <%= diet.body.hits.length %> results</h4>
            <br>
            <div class="row">
             <% diet.body.hits.forEach(function(nutrition)
             { %>
                 <div class="col-lg-4">
                      <div class="card mb-3">
  <img id="photo" src="<%= nutrition.recipe.image %>" class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title"><%= nutrition.recipe.label %></h5>
     <p><%= nutrition.recipe.healthLabels[nutrition.recipe.healthLabels.length-1] %></p>
        <p><strong>Calories - </strong><%= nutrition.recipe.calories %></p>
        <p><strong>Fat - </strong><%= nutrition.recipe.totalNutrients.FAT.quantity %> <%=nutrition.recipe.totalNutrients.FAT.unit %></p>
        <p><strong>Carbohydrate - </strong><%= nutrition.recipe.totalNutrients.CHOCDF.quantity %> <%=nutrition.recipe.totalNutrients.CHOCDF.unit %></p>
        <p><strong>Protein - </strong><%= nutrition.recipe.totalNutrients.PROCNT.quantity %> <%=nutrition.recipe.totalNutrients.PROCNT.unit %></p>
      
        <p><strong>Sodium - </strong><%= nutrition.recipe.totalNutrients.NA.quantity %> <%=nutrition.recipe.totalNutrients.NA.unit %></p>
        <p><strong>Calcium - </strong><%= nutrition.recipe.totalNutrients.CA.quantity %> <%=nutrition.recipe.totalNutrients.CA.unit %></p>
        <p><strong>Magnesium - </strong><%= nutrition.recipe.totalNutrients.MG.quantity %> <%=nutrition.recipe.totalNutrients.MG.unit %></p>
        <p><strong>Potassium - </strong><%= nutrition.recipe.totalNutrients.K.quantity %> <%=nutrition.recipe.totalNutrients.K.unit %></p>
        <p><strong>Iron - </strong><%= nutrition.recipe.totalNutrients.FE.quantity %> <%=nutrition.recipe.totalNutrients.FE.unit %></p>
        <p><strong>Vitamin A - </strong><%= nutrition.recipe.totalNutrients.VITA_RAE.quantity %> <%=nutrition.recipe.totalNutrients.VITA_RAE.unit %></p>
       
        <p><strong>Vitamin E - </strong><%= nutrition.recipe.totalNutrients.TOCPHA.quantity %> <%=nutrition.recipe.totalNutrients.TOCPHA.unit %></p>
        <p><strong>Vitamin K - </strong><%= nutrition.recipe.totalNutrients.VITK1.quantity %> <%=nutrition.recipe.totalNutrients.VITK1.unit %></p>
       
        
               <a href="<%= nutrition.recipe.url %>"><strong>Know about recipe</strong></a>
  </div>
</div>
                 </div>
             <% }) %>  
            </div>
        </div>
      
<hr>
